{
  "name": "personal-portfolio-project",
  "description": "-Building a 4 page portfolio that will showcase your past work and lets your co-workers/classmates know a little bit more about yourself",
  "scripts": {
    "build:css": "cleancss src/css/*.css -o dist/css/styles.min.css",
    "copy:html": "cp src/*.html dist",
    "copy:innerhtml": "mkdir -p dist/html && cp src/html/*.html dist",
    "build:js": "uglifyjs dist/js/bundle.js --compress -o dist/js/bundle.min.js",
    "imagemin": "imagemin src/images/* -o dist/images",
    "rollup": "rollup src/js/main.js --file dist/js/bundle.js --format es",
    "build": "npm run build:css && npm run copy:html && npm run copy:innerhtml && npm run rollup && npm run build:js && npm run imagemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaxWiens28/final-project.git"
  },
  "author": "Max Wiens <mjwiens@yahoo.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MaxWiens28/final-project/issues"
  },
  "homepage": "https://github.com/MaxWiens28/final-project#readme",
  "devDependencies": {
    "clean-css-cli": "^5.5.2",
    "imagemin-cli": "^7.0.0",
    "mkdir": "^0.0.2",
    "rollup": "^2.70.0",
    "uglify-js": "^3.15.2"
  }
}
